            "item": [
                {
                    "name": "Obtain Access Token",
                    "request": {
                        "url": {
                            "host": "{{baseUrl}}",
                            "path": "oauth\/token",
                            "query": [],
                            "raw": "{{baseUrl}}\/oauth\/token"
                        },
                        "method": "POST",
